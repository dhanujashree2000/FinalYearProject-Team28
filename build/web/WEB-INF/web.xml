        <servlet-name>u3</servlet-name>
        <url-pattern>/u3</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>u5</servlet-name>
        <url-pattern>/u5</url-pattern>
    </servlet-mapping>
