        <servlet-class>i2</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>u1</servlet-name>
        <servlet-class>u1</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>u3</servlet-name>
        <servlet-class>u3</servlet-class>
